    var e = options.e, jsCode = options.jsCode, serverSide = options.serverSide;
    console.error('Exception in rendering!');
    var msg = handleRenderFunctionIssue(options);
    if (jsCode) {
        console.error("JS code was: " + jsCode);
    }
