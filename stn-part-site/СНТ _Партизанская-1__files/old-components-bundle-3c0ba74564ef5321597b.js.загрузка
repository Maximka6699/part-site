var isSameUrl = function (a, b) {
    var typeA = typeof a;
    var typeB = typeof b;
    if (typeA === 'string' && typeB === 'string' && a === b) {
        return true;
    }
    if (typeA === 'object' && typeB === 'object') {
        return (a.length === b.length &&
            a.every(function (v, i) { return v === b[i]; }));
    }
    return false;
};

